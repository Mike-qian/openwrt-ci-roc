### 通用配置文件 ###

CONFIG_DEVEL=y
CONFIG_TARGET_OPTIONS=y
CONFIG_TARGET_OPTIMIZATION="-O3 -pipe -mtune=cortex-a53 -mcpu=cortex-a53 -fomit-frame-pointer -fno-plt -fuse-linker-plugin --param=max-unroll-times=4 -frename-registers -falign-functions=32 -ftree-vectorize -fvect-cost-model=dynamic -mfix-cortex-a53-835769 -mfix-cortex-a53-843419 -DUSE_PTHREAD -mno-outline-atomics -ffunction-sections -fdata-sections -Wl,--gc-sections"
CONFIG_CCACHE=y
CONFIG_USE_LTO_THIN=y
CONFIG_USE_MOLD=y
CONFIG_USE_GC_SECTIONS=y
CONFIG_PACKAGE_default-settings=y
CONFIG_PACKAGE_default-settings-chn=y
CONFIG_TARGET_PER_DEVICE_ROOTFS=y
CONFIG_LUCI_LANG_zh_Hant=y

